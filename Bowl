function readCookie(name) {
    name += '=';
    for (var ca = document.cookie.split(/;\s*/), i = ca.length - 1; i >= 0; i--)
        if (!ca[i].indexOf(name))
            return ca[i].replace(name, '');
}

<!-- Langue  A FAIRE GET cookie puis get nav en second choix -->
var userLang = navigator.language || navigator.userLanguage; 
//alert ("The language is: " + userLang); // A FAIRE : 


function ajouter_a_mes_jeux_confirmation() {
	
	if(are_cookies_enabled()) {
		if(userLang == 'fr-FR') document.getElementById('ajouter_a_mes_jeux_confirmation').innerHTML = "Ajouté dans <a href=\"categorie-jeux.php?cat=mes-jeux\">Mes Jeux</a>";
		else document.getElementById('ajouter_a_mes_jeux_confirmation').innerHTML = "<a href=\"categorie-jeux.php?cat=mes-jeux\">OK !</a>";
		var conf_delay = 1800;
	}
	else {
		if(userLang == 'fr-FR') document.getElementById('ajouter_a_mes_jeux_confirmation').innerHTML = "<img src=\"images\/error2.png\"> <span class=\"red\">Veuillez <a href=\"categorie-jeux.php?cat=mes-jeux\" target=\"_blank\">activer les cookies de votre navigateur<\/a><\/span>";
		else document.getElementById('ajouter_a_mes_jeux_confirmation').innerHTML = "<img src=\"images\/error2.png\"> <span class=\"red\">Please <a href=\"c.php?p=premiers-pas\" target=\"_blank\">enable cookies in your browser<\/a><\/span>";
		var conf_delay = 5000;
	}
	
	var tous_les_jeux_tab = readCookie('Visites');
	var tous_les_jeux_tab = tous_les_jeux_tab.split("+");
	var nb_mes_jeux = tous_les_jeux_tab.length;
	if(test == 2) alert('nb_mes_jeux:'+nb_mes_jeux);

	/* jeux vide ? */
	var empties = tous_les_jeux_tab.length - tous_les_jeux_tab.filter(String).length;
	var nb_mes_jeux_no_empties = nb_mes_jeux - empties;
	if(test == 2) alert('nb_mes_jeux:'+nb_mes_jeux+' - empties:'+empties+' = nb_mes_jeux_no_empties:'+nb_mes_jeux_no_empties+' - tous_les_jeux_tab:'+tous_les_jeux_tab);
	var nb_mes_jeux = nb_mes_jeux_no_empties;

	//document.getElementById('mesjeux_nb_jeux').innerHTML='('+nb_mes_jeux+')';
	
	if(nb_mes_jeux >= 1) {
		if(nb_mes_jeux >= 100) document.getElementById('mesjeux_nb_jeux').innerHTML='(+99)';
		else document.getElementById('mesjeux_nb_jeux').innerHTML='('+nb_mes_jeux+')';
	}
	else document.getElementById('mesjeux_nb_jeux').innerHTML='';
	
	document.getElementById('ajouter_a_mes_jeux_confirmation').style.opacity = 1;
	var opacity = 1;
	window.setTimeout(function() { fader(opacity); } , conf_delay);
}



function supprimer_de_mes_jeux_confirmation() { //  A FAIRE 0 : Bug : si déjà ajouté dans mes jeux puis suppression dans la foulée : erreur affichage confirmation.
	
	if(userLang == 'fr-FR') document.getElementById('ajouter_a_mes_jeux_confirmation').innerHTML = "Supprimé de <a href=\"categorie-jeux.php?cat=mes-jeux\">Mes Jeux</a>";
	else document.getElementById('ajouter_a_mes_jeux_confirmation').innerHTML = "<a href=\"categorie-jeux.php?cat=mes-jeux\">OK !</a>";
	
	var tous_les_jeux_tab = readCookie('Visites');
	var tous_les_jeux_tab = tous_les_jeux_tab.split("+");
	var nb_mes_jeux = tous_les_jeux_tab.length;
	if(test == 2) alert('nb_mes_jeux:'+nb_mes_jeux);
	
	/* jeux vide ? */
	var empties = tous_les_jeux_tab.length - tous_les_jeux_tab.filter(String).length;
	var nb_mes_jeux_no_empties = nb_mes_jeux - empties;
	if(test == 2) alert('nb_mes_jeux:'+nb_mes_jeux+' - empties:'+empties+' = nb_mes_jeux_no_empties:'+nb_mes_jeux_no_empties+' - tous_les_jeux_tab:'+tous_les_jeux_tab);
	var nb_mes_jeux = nb_mes_jeux_no_empties;
	
	document.getElementById('mesjeux_nb_jeux').innerHTML='('+nb_mes_jeux+')';

	document.getElementById('ajouter_a_mes_jeux_confirmation').style.opacity = 1;
	var opacity = 1;
	window.setTimeout(function() { fader(opacity); } , 1800);
}



function trop_de_jeux_confirmation() {
	
	if(userLang == 'fr-FR') document.getElementById('ajouter_a_mes_jeux_confirmation').innerHTML = "Trop de jeux dans <a href=\"categorie-jeux.php?cat=mes-jeux\">Mes Jeux</a>";
	else document.getElementById('ajouter_a_mes_jeux_confirmation').innerHTML = "Too much games in <a href=\"categorie-jeux.php?cat=mes-jeux\">My Games</a>";
	
	document.getElementById('mesjeux_nb_jeux').innerHTML='<span style="color:#FF0000;"('+nb_mes_jeux+')</span>';
	document.getElementById('ajouter_a_mes_jeux_confirmation').style.opacity = 1;
	var opacity = 1;
	window.setTimeout(function() { fader(opacity); } , 1800);
}



function fader(opacity) {
	var opacity = opacity-0.03;
	opacity.toFixed(2);
	document.getElementById('ajouter_a_mes_jeux_confirmation').style.opacity = opacity;
	if(opacity>=0) { window.setTimeout(function() { fader(opacity); } , 40); }
}



function getCookie_mj(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) {
			return c.substring(name.length,c.length);
		}
    }
    return "";
}



function checkCookie_mj(id_jeu) {
    var all_id = getCookie_mj("Visites");
	var all_id = all_id.replace(/ {2,}/g, ' ');
	if (test == 2) alert("Jeux id (checkCookie_mj) : " + all_id);
	return all_id;
}



function readCookie_mj(id_jeu) {

	var new_game_id = escape(id_jeu);
	if(isNaN(new_game_id) == true) return; // sécurité
	
	// 1- On vérifie les jeux déjà présents.
    var all_games = checkCookie_mj();
	
	// 2- On met ces jeux dans un tableau
	if(all_games != '') {
		if (test == 2) alert('1- Cookie existant - all_games : "'+all_games+'"');
		var all_games_tab = all_games.split("+");
		if(all_games_tab.indexOf(new_game_id) > -1) {		
			if (test == 2) alert('2- '+new_game_id+' est déjà dans Mes Jeux.');
			document.getElementById('ajouter_a_mes_jeux').style.backgroundPosition='-16px 0'; 
			document.getElementById('ajouter_a_mes_jeux').title = 'Ce jeu fait parti de vos jeux favoris.';
		}
	}
}



<!-- Ajouter au favoris COOKIE -->
/*
function favorisadd(id) {

var base_url = url.substring(0, xend);
url = base_url + 'cookies_add.php?id=' + escape(id);
newwindow=window.open(
url,'name2', 'status=no, toolbar=no, scrollbars=yes, titlebar=no, menubar=no, resizable=no, width=1, height=1, directories=no, location=no');

//document.getElementById('jeu_id_box').innerHTML='<strong>You changed me</strong>';
document.getElementById('jeuxajoute').innerHTML='<strong>Jeu ajout&eacute; &agrave; <a href="mes-jeux.php" title="Afficher mes jeux favoris">Mes Jeux</a> !</strong><br>';

if (window.focus) {newwindow.focus()}
return false;

}



