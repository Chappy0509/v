function Resize(anim,id_games,size,widthorigine,heightorigine) {
	
	if(test == 1) alert('Resize > variables - anim : '+anim+' - id_games : '+id_games+' - size : '+size+' - widthorigine : '+widthorigine+' - heightorigine : '+heightorigine+ ' - Domain name : '+window.location.hostname);

	/* Hauteur additionnelle proportionnelle */
	var sizeh = (size/widthorigine)*heightorigine;
	
	// vérouillage de la taille maximale
	var screenwidthmax = screen.availWidth;
	//alert(screenwidthmax);
	
   // Zoom - ou + Flash object named "anim" or Div named "flash"
   if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){
   	   var width = document.getElementById('flash').style.width;
	   var height= document.getElementById('flash').style.height;

	   // If 100% ajust to original sizes
	   if(width=='100%') {
		   var width = widthorigine;
		   var height = heightorigine;
	   }

	   var width = parseInt(width,10)+ parseInt(size,10);
	   var height = parseInt(height,10)+ parseInt(sizeh,10);

	   if(width>330 && width<screenwidthmax) {
	   document.getElementById('flash').style.width = width+'px'; void(0); 
	   document.getElementById('flash').style.height = height+'px'; void(0); 
	   }
	   else { 
	   		//alert(width);
	   		width = parseInt(width)-parseInt(size);
	   		height = parseInt(height)-parseInt(sizeh);
	   		//alert(width);
	   }
   }

   else if(/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { 
   	   var width = document.getElementById(anim).width;
	   var height= document.getElementById(anim).height;

	   // If 100% ajust to original sizes
	   if(width=='100%') {
		   var width = widthorigine;
		   var height = heightorigine;
	   }
	   var width = parseInt(width,10)+ parseInt(size,10);
	   var height = parseInt(height,10)+ parseInt(sizeh,10);
	   
	   if(width>340 && width<screenwidthmax) {
	   document.getElementById(anim).width = width; void(0); 
	   document.getElementById(anim).height = height; void(0); 
	   }
	   else { 
	   		//alert(width);
	   		width = parseInt(width)-parseInt(size);
	   		height = parseInt(height)-parseInt(sizeh);
	   		//alert(width);
	   }
   }
   
   else {

		var width = document.getElementById('flash').style.width;
		var height= document.getElementById('flash').style.height;
		
		var width = parseInt(width,10)+ parseInt(size,10);
		var height = parseInt(height,10)+ parseInt(sizeh,10);
		
		if(width>340 && width<screenwidthmax) {
		   document.getElementById('flash').style.width = width+'px'; void(0); 
		   document.getElementById('flash').style.height = height+'px'; void(0); 
		}
		else { 
			//alert(width);
			width = parseInt(width)-parseInt(size);
			height = parseInt(height)-parseInt(sizeh);
			if(test == 1) alert('Resize > Taille jeu dépasse l\'écran : '+width+' - '+height);
		}

   } // A FAIRE : bloquer si dépasse l'écran.
   // A FAIRE : sécurité générale

	// Add cookies
	var concatenation = id_games+"|"+width+"|"+height;
	//alert('concatenation:'+concatenation);
	var pathname=location.pathname;
	var myDomain=pathname.substring(0,pathname.lastIndexOf('/')) +'/';
	var date_exp = new Date();
	date_exp.setTime(date_exp.getTime()+(3650*24*3600*1000));
	// 365 days
	SetCookie(id_games,concatenation,date_exp,myDomain);

   	// Add cookies > taille auto : "Off"
	var concatenation_autosize_on_off = id_games+'|0';
	SetCookie('auto_size_'+id_games,concatenation_autosize_on_off,date_exp,myDomain);	
	if(test == 1) alert('Resize > Cookie auto_size_'+id_games+' get "Off" : '+concatenation+' - date_exp : '+date_exp+' + myDomain : '+myDomain);

	// Auto_size = "off"
	document.getElementById("survol_zauto_id").className="survol_zauto";

	// Centrage du jeu
	//alert('GameCentrage:'+width);
	GameCentrage(width);

}



function Resize_embed(anim,id_games,size,widthorigine,heightorigine) {

	if(test == 1) alert('Resize_embed > variables - anim : '+anim+' - id_games : '+id_games+' - size : '+size+' - widthorigine : '+widthorigine+' - heightorigine : '+heightorigine);

	/* Hauteur additionnelle proportionnelle */
	var sizeh = (size/widthorigine)*heightorigine;
	
	// vérouillage de la taille maximale
	var screenwidthmax = screen.availWidth;
	//alert(screenwidthmax);
	
   // Zoom - ou + Flash object named "anim" or Div named "flash"
   if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){
		var width = document.getElementById('flash').style.width;
	   var height= document.getElementById('flash').style.height;

	   // If 100% ajust to original sizes
	   if(width=='100%') {
		   var width = widthorigine;
		   var height = heightorigine;
	   }

	   var width = parseInt(width,10)+ parseInt(size,10);
	   var height = parseInt(height,10)+ parseInt(sizeh,10);

	   if(width>330 && width<screenwidthmax) {
	   document.getElementById('flash').style.width = width+'px'; void(0); 
	   document.getElementById('flash').style.height = height+'px'; void(0); 
	   }
	   else { 
	   		//alert(width);
	   		width = parseInt(width)-parseInt(size);
	   		height = parseInt(height)-parseInt(sizeh);
	   		//alert(width);
	   }
   }

   else if(/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { 
		var width = document.getElementById(anim).width;
	   var height= document.getElementById(anim).height;

	   // If 100% ajust to original sizes
	   if(width=='100%') {
		   var width = widthorigine;
		   var height = heightorigine;
	   }
	   var width = parseInt(width,10)+ parseInt(size,10);
	   var height = parseInt(height,10)+ parseInt(sizeh,10);
	   
	   if(width>340 && width<screenwidthmax) {
	   document.getElementById(anim).width = width; void(0); 
	   document.getElementById(anim).height = height; void(0); 
	   }
	   else { 
	   		//alert(width);
	   		width = parseInt(width)-parseInt(size);
	   		height = parseInt(height)-parseInt(sizeh);
	   		//alert(width);
	   }
   }
   
   else {
		
		var width = document.getElementById('flash').style.width;
		var height= document.getElementById('flash').style.height;
		
		var width = parseInt(width,10)+ parseInt(size,10);
		var height = parseInt(height,10)+ parseInt(sizeh,10);
		
		if(width>340 && width<screenwidthmax) {
		   document.getElementById('flash').style.width = width+'px'; void(0); 
		   document.getElementById('flash').style.height = height+'px'; void(0); 
		}
		else { 
			//alert(width);
			width = parseInt(width)-parseInt(size);
			height = parseInt(height)-parseInt(sizeh);
			if(test == 1) alert('Resize > Taille jeu dépasse l\'écran : '+width+' - '+height);
		}

   }
   // A FAIRE : bloquer si dépasse l'écran.
   // A FAIRE : sécurité générale
	
	
	// Add cookies
	var concatenation = width+"|"+height;
	//alert('concatenation:'+concatenation);
	var pathname=location.pathname;
	var myDomain=pathname.substring(0,pathname.lastIndexOf('/')) +'/';
	var date_exp = new Date();
	date_exp.setTime(date_exp.getTime()+(3650*24*3600*1000));
	// 365 days
	SetCookie('auto_size_embed',concatenation,date_exp,myDomain);

	document.getElementById('embed_size_display').innerHTML = width+' X '+height;

	if(test == 1) alert('Resize_embed > Cookie auto_size_embed OK, concatenation : '+concatenation+' - date_exp : '+date_exp+' + myDomain : '+myDomain);

	// Auto_size = "off"
	//document.getElementById("survol_zauto_id").className="survol_zauto";

	// Centrage du jeu
	//alert('GameCentrage:'+width);
	//GameCentrage(width);

}



function GetSize(id_games,game_size_width_dev,game_size_height_dev) {
	
	//var test=1;
	
	// Embed
	if(id_games == 'embed') {
		
		if(test == 1) alert('Getsize_embed ( '+id_games+' - '+game_size_width_dev+' - '+game_size_height_dev+' )');
		
		var auto_size_embed = GetCookie('auto_size_embed');
		
		if (auto_size_embed) {
			var b = auto_size_embed.split('|');
			var width = b[0];
			var height = b[1];

			if(test == 1) alert('width : '+width+' - height : '+width);
		
			if(isNaN(width) || isNaN(height)) {

				if(test == 1) alert('width ou height not exists');

				var width = game_size_width_dev;
				var height = game_size_height_dev;

				if(test == 1) alert('width : '+width+' - height : '+height);

			}
											
		}
		else {
			var width = game_size_width_dev;
			var height = game_size_height_dev;
		}
		
		document.getElementById('embed_size_display').innerHTML = width+' X '+height;

		if(test == 1) alert('auto_size_embed_width : '+width+' - auto_size_embed_height : '+height);

		var navigateur = navigator.userAgent.toLowerCase();

		if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){ // A FAIRE 0 : les autres agents (nouveau script)
			//   alert("ff "+width+" - "+height)
			document.getElementById('flash').style.width = width+'px'; void(0); 
			document.getElementById('flash').style.height = height+'px'; void(0); 
		}

		else if(/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { 
			document.getElementById('anim').width = width; void(0); 
			document.getElementById('anim').height = height; void(0); 
		}

		else {
			document.getElementById('flash').style.width = width+'px'; void(0); 
			document.getElementById('flash').style.height = height+'px'; void(0); 
			//alert('Mise a jour de la taille OK.');
		}

		if(test == 1) alert('Taille ajustée au démarrage');

		// Centrage du jeu
		GameCentrage(width);
		if(test == 1) alert('Jeu centré');

	} // Embed:fin
	else {
	
		if(test == 1) alert('GetSize > variables - id_games : '+id_games+' - game_size_width_dev : '+game_size_width_dev+' - game_size_height_dev : '+game_size_height_dev);
		  
		//if(test == 1) alert('auto_size == 0');
		//auto_size = 0;
		
		// Auto_sizing : 0 ou 1
		var auto_size_id_games = GetCookie('auto_size_'+id_games);
		if (auto_size_id_games) {
			if(test == 1) alert('GetSize > Cookie Autosize brut : '+auto_size_id_games);		
			var b = auto_size_id_games.split('|');
			var auto_size = b[1];
			if(test == 1) alert('GetSize > Cookie Autosize >  extracted - id_games : '+id_games+' - auto_size : '+auto_size);		
		}
		else {
			auto_size = 0;
		}
		//if(test == 1) alert('Kikoo'+b+' - auto_size_id_games : '+auto_size_id_games);
		//if(test == 1) alert('GetSize > Autosize > cookie auto_size - auto_size : '+auto_size);
	
		if (auto_size_id_games && auto_size == 1) {
	
			if(test == 1) alert('auto_size_id_games : '+auto_size_id_games+' - auto_size : '+auto_size);		

			Fullsize('anim',id_games,game_size_width_dev,game_size_height_dev);
			var b = auto_size_id_games.split('|');
			var auto_size = b[1];
			// A FAIRE > sécurité de taille : si le cookie donne une taille minuscule, ou invraissemblable alors mettre taille par défaut.	
	
		}
		else {
		
			if(test == 1) alert('GetSize > Cookie Autosize n\'existe pas ou auto_size=0 : on prend le cookie manuel ou les dimensions par défaut.');
	
			size_id_games=GetCookie(id_games);
					
			if (size_id_games && size_id_games.indexOf('undefined') == -1) {
				
				if(test == 1) alert('GetSize > Taille > Cookie manuel : '+size_id_games+' - size_id_games.indexOf > undefined : '+size_id_games.indexOf('undefined'));
				
				//document.getElementById('size').innerHTML='<span style="save_load"><strong><a href="/" title="Save">'+le_prenom+' !</a></strong><br></span>'; 
				a=size_id_games.split('|');
				var id_games = a[0];
				var width = a[1];
				var height = a[2];
		
				if(test == 1) alert('GetSize > cookies size_id_games - id_games : '+id_games+' - width : '+width+' - height : '+height+' - size_id_games : '+size_id_games);
		
				var navigateur = navigator.userAgent.toLowerCase();
		
				if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){
					//   alert("ff "+width+" - "+height)
					document.getElementById('flash').style.width = width+'px'; void(0); 
					document.getElementById('flash').style.height = height+'px'; void(0); 
				}
		
				else if(/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { 
					document.getElementById('anim').width = width; void(0); 
					document.getElementById('anim').height = height; void(0); 
				}
		
				else {
					document.getElementById('flash').style.width = width+'px'; void(0); 
					document.getElementById('flash').style.height = height+'px'; void(0); 
					//alert('Mise a jour de la taille OK.');
				}
						
				// Centrage du jeu
				GameCentrage(width);
	
			}
		
			// A FAIRE : Si pas de cookies, on prend les valeur par défaut ajustée à 968 de la taille du jeu
			else {
	
				if(test == 1) alert('GetSize > Taille > Par défaut (968x561) : '+game_size_width_dev+' - '+game_size_height_dev);
				
				// EN COURS A FAIRE : faire une fonction de resize default
				function OptimaleGameDefaultSize (game_size_width_dev,game_size_height_dev) { // Game > optimal size
					
					var game_width = game_size_width_dev;
					var game_height = game_size_height_dev;
					
					// On ajuste la taille optimale à une largeur de 968px et d'une hauteur à définir 
					var game_height = (game_height/game_width)*968;
					var game_height = Math.floor(game_height);
					var game_width = 968;
	
					if(isNaN(game_size_width_dev) == true || isNaN(game_size_height_dev) == true) {
						var default_size = '968|561';
						if(test == 1) alert('GetSize > Taille > Function OptimaleGameDefaultSize() > game_size_width_dev:'+isNaN(game_size_width_dev)+' ou game_size_width_dev:'+isNaN(game_size_height_dev)+' n\'est pas un nombre : '+default_size+'. On prend la taille par défaut dure : '+default_size);
					}
	
					else {
						if(test == 1) alert('GetSize > Taille > Function OptimaleGameDefaultSize() > Default_size est un nombre : '+default_size);
						var default_size = game_width+'|'+game_height;
					}
					
					if(test == 1) alert('GetSize > Taille > Function OptimaleGameDefaultSize() > Default_size : '+default_size);
					
					return default_size;
	
				}
				
				var default_size = OptimaleGameDefaultSize(game_size_width_dev,game_size_height_dev);
				var default_size = default_size.split('|');
				var default_width = default_size[0];
				var default_height = default_size[1];
	
				if(test == 1) alert('GetSize > Taille > Par défaut réajusté (968x???) : default_width : '+default_width+' - default_height : '+default_height);
			
				// alert("Cookies OFF : "+id_games+" - "+width+" - "+height);
				/*
				if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){
					// alert("ff "+width+" - "+height)
					document.getElementById('flash').style.width = '968px'; void(0); 
					document.getElementById('flash').style.height = '561px'; void(0); 
				}
			
				else if(/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { 
					document.getElementById('anim').width = 968; void(0); 
					document.getElementById('anim').height = 561; void(0); 
				}
			   
				else {
					document.getElementById('flash').style.width = '968px'; void(0); 
					document.getElementById('flash').style.height = '561px'; void(0); 
					//alert('Mise a jour de la taille OK. cookie.');
				}
				*/
				
				// A FAIRE : si width ou height don't exists > défault 968x561
				// A FAIRE : mettre la fonction "OptimaleGameDefaultSize" en indépendante.
				
				if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){ // A FAIRE : utiliser la fonction
					// alert("ff "+width+" - "+height)
					document.getElementById('flash').style.width = default_width+'px'; void(0); 
					document.getElementById('flash').style.height = default_height+'px'; void(0); 
				}
			
				else if(/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { 
					document.getElementById('anim').width = default_width; void(0); 
					document.getElementById('anim').height = default_height; void(0); 
				}
			   
				else {
					document.getElementById('flash').style.width = default_width+'px'; void(0); 
					document.getElementById('flash').style.height = default_height+'px'; void(0); 
					//alert('Mise a jour de la taille OK. cookie.');
				}
				
				// Centrage du jeu
				GameCentrage(default_width);
	
			}
			
		}
	}
}



function ResizeOrigine(anim,id_games,width,height) { // A FAIRE : width dev et height dev > 968 x height ajustée
     
	if(test == 1) alert('ResizeOrigine > variables - anim:'+anim+' - id_games : '+id_games+' - width : '+width+' + height : '+height);

	// On ajuste la taille optimale à une largeur de 968px et d'une hauteur à définir 
	var height = (height/width)*968;
	var height = Math.floor(height);
	var width = 968; /*808*/

	if(test == 1) alert('ResizeOrigine > variables ajustées - width : '+width+' + height : '+height);

	var navigateur = navigator.userAgent.toLowerCase();
	
	if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){ // Firefox
		document.getElementById('flash').style.width = width+'px'; void(0); 
		document.getElementById('flash').style.height = height+'px'; void(0); 
	}
	
	else if(/MSIE[\/\s](\d+\.\d+)/.test(navigator.userAgent)) { // Internet Explorer
		document.getElementById('anim').width = width; void(0); 
		document.getElementById('anim').height = height; void(0); 
	}
	
	else { // Chrome, etc.
		document.getElementById('flash').style.width = width+'px'; void(0); 
		document.getElementById('flash').style.height = height+'px'; void(0); 
	}
  
   	// Add cookies > taille manuelle
	var concatenation = id_games+"|"+width+"|"+height;
	/*setCook("size",concatenation,365);*/
	var pathname=location.pathname;
	var myDomain=pathname.substring(0,pathname.lastIndexOf('/')) +'/';
	var date_exp = new Date();
	date_exp.setTime(date_exp.getTime()+(3650*24*3600*1000));
	// Ici on définit une durée de vie de 365 jours
	SetCookie(id_games,concatenation,date_exp,myDomain);

	if(test == 1) alert('ResizeOrigine > Cookie size - id_games:'+id_games+' - concatenation : '+concatenation+' - date_exp : '+date_exp+' + myDomain : '+myDomain);

   	// Add cookies > taille auto : "Off"
	var concatenation_autosize_on_off = id_games+'|0';
	SetCookie('auto_size_'+id_games,concatenation_autosize_on_off,date_exp,myDomain);	

	if(test == 1) alert('ResizeOrigine > Cookie auto_size - auto_size_id_games: auto_size_'+id_games+' - concatenation_autosize_on_off : '+concatenation_autosize_on_off+' - date_exp : '+date_exp+' + myDomain : '+myDomain);
	
	// Auto_size = "off"
	document.getElementById("survol_zauto_id").className="survol_zauto";

	// Centrage du jeu
	GameCentrage(width);

}


// A FAIRE 0 : opera / safari et autres ainsi que pour le zoom et autres fonctionnalités.
function Fullsize(anim,id_games,game_size_width_dev,game_size_height_dev) {
		
	if(test == 1) alert('Fullsize > anim : '+anim+' - id_games : '+id_games+' - game_size_width_dev : '+game_size_width_dev+' - game_size_height_dev : '+game_size_height_dev);
	
	// Taille optimale selon le développeur
	if(!isNaN(game_size_width_dev) == true || !isNaN(game_size_height_dev) == true) {
		if(test == 1) alert('Fullsize > Entier : TRUE - game_size_width_dev : '+game_size_width_dev+' - game_size_height_dev ajusté : '+game_size_height_dev);
		var game_width = game_size_width_dev;
		var game_height = game_size_height_dev;
		// On ajuste la taille optimale à une largeur de 968px et d'une hauteur à définir 
		var game_height = (game_height/game_width)*968;
		var game_height = Math.floor(game_height);
		var game_width = 968;
	}
	else {
		if(test == 1) alert('Fullsize > Entier : FALSE > Taille par défaut manuelle (968x561)');
		var game_width = parseInt(968,10);
		var game_height = parseInt(561,10);
	} 

	if(test == 1) alert('Fullsize > gamewidth ajusté : '+game_width+' - gameheight ajusté : '+game_height);

	var navigateur = navigator.userAgent.toLowerCase();
	
	var browser = browserCheckin();
   	
	if(browser == "Firefox" || browser == "Chrome" || browser == "Opera" || browser == "Safari" || browser == "InternetExplorer") {
	
		var window_width = window.innerWidth;
			
			if(window.innerWidth <= 735) var window_width_available = window_width-40;
			else var window_width_available = window_width-555;
			
			//alert('window_width_available:'+window_width_available)
		
		var window_height = window.innerHeight;
		var window_height_available = window_height-200;
		//var screen_ratio = window_width_available/window_height_available;
		
		var ratio_dev = game_size_width_dev / game_size_height_dev;
		var ratio_screen = window_width_available / window_height_available;
		
		if(test == 1) alert('Fullsize > window_width_available : '+window_width_available+' - window_height_available : '+window_height_available+' - ratio_dev : '+ratio_dev+' - ratio_screen : '+ratio_screen); 
			
		if(window_width_available >= window_height_available  && ratio_dev <= ratio_screen) { //&& screen_ratio > 1.40
			if(test == 1) alert('Fullsize > Base : hauteur : (window_width_available:'+window_width_available+' >= window_height_available:'+window_height_available);
			//var game_height_new = window_height-200; //300
			var game_height_new = window_height_available; //300
			var game_width_new = (game_width/game_height)*game_height_new;
			var game_width_new = Math.floor(game_width_new);
		}
		else {
			if(test == 1) alert('Fullsize > Base : largeur (window_width_available : '+window_width_available+' < window_height_available : '+window_height_available+')'); // ou screen_ratio : '+screen_ratio+' < 1.40 
			
				if(window.innerWidth <= 735) var game_width_new = window_width-40;
				else var game_width_new = window_width-555;
			
			var game_height_new = (game_height/game_width)*game_width_new;
			var game_height_new = Math.floor(game_height_new);
		}
	
		//if(test == 1) alert('Fullsize > Base : hauteur : (game_width_new:'+game_width_new+' >= game_height_new:'+game_height_new);
	
		document.getElementById('flash').style.width = game_width_new+'px'; void(0); 
		document.getElementById('flash').style.height = game_height_new+'px'; void(0); 
		document.getElementById('flash').style.textAlign = 'left'; 
	
		// Centrage du jeu
		GameCentrage(game_width_new);
		
		// Onload
		document.getElementById("survol_zauto_id").className="survol_zauto_on";
	}
	
	//else if() OPERA A FAIRE 0
	//else if() SAFARI A FAIRE 0
	
	else { // "undefined" on utilise le Fullsize de Chrome

		var window_width = window.innerWidth;
		
		if(window.innerWidth <= 735) var window_width_available = window_width-30;
		else var window_width_available = window_width-545;
		
		var window_height = window.innerHeight;
		var window_height_available = window_height-200;
		//var screen_ratio = window_width_available/window_height_available;
		
		var ratio_dev = game_size_width_dev / game_size_height_dev;
		var ratio_screen = window_width_available / window_height_available;
		
		if(test == 1) alert('Fullsize > window_width_available : '+window_width_available+' - window_height_available : '+window_height_available+' - ratio_dev : '+ratio_dev+' - ratio_screen : '+ratio_screen); 
			
		if(window_width_available >= window_height_available  && ratio_dev <= ratio_screen) { //&& screen_ratio > 1.40
			if(test == 1) alert('Fullsize > Base : hauteur : (window_width_available:'+window_width_available+' >= window_height_available:'+window_height_available);
			//var game_height_new = window_height-200; //300
			var game_height_new = window_height_available; //300
			var game_width_new = (game_width/game_height)*game_height_new;
			var game_width_new = Math.floor(game_width_new);
		}
		else {
			if(test == 1) alert('Fullsize > Base : largeur (window_width_available : '+window_width_available+' < window_height_available : '+window_height_available+')'); // ou screen_ratio : '+screen_ratio+' < 1.40 
			
			if(window.innerWidth <= 735) var game_width_new = window_width-30;
			else var game_width_new = window_width-545;
			
			var game_height_new = (game_height/game_width)*game_width_new;
			var game_height_new = Math.floor(game_height_new);
		}
	
		//if(test == 1) alert('Fullsize > Base : hauteur : (game_width_new:'+game_width_new+' >= game_height_new:'+game_height_new);
	
		document.getElementById('flash').style.width = game_width_new+'px'; void(0); 
		document.getElementById('flash').style.height = game_height_new+'px'; void(0); 
		document.getElementById('flash').style.textAlign = 'left'; 
	
		// Centrage du jeu
		GameCentrage(game_width_new);
		
		// Onload
		document.getElementById("survol_zauto_id").className="survol_zauto_on";
  }

	// Add cookies
	var concatenation = id_games+"|"+game_width_new+"|"+game_height_new;
	var pathname=location.pathname;
	var myDomain=pathname.substring(0,pathname.lastIndexOf('/')) +'/';
	var date_exp = new Date();
	date_exp.setTime(date_exp.getTime()+(3650*24*3600*1000));
	// Ici on définit une durée de vie de 365 jours
	SetCookie(id_games,concatenation,date_exp,myDomain);

}



function GameCentrage(game_width) {
	var window_width = window.innerWidth;
	var game_margin_left = (window_width/2)-507-(0.5*game_width);
	var game_margin_left = Math.floor(game_margin_left);
	if(game_margin_left<0) var game_margin_left = 0;
	document.getElementById('id_contener_general_jeux').style.marginLeft = game_margin_left+'px'; 
	if(test == 1) alert('GameCentrage (game_width:'+game_width+') OK > game_margin_left : '+game_margin_left+'.px');
}



function Autosize(anim,id_games,game_size_width_dev,game_size_height_dev) { // Auto_size : switch "On" <> "Off"
	// A FAIRE : changer title : Auto size "activé" si actif OmO
	// A FAIRE : bug FF - trop grand
	// Cookies Autosize = 1 ou 0 ?
	auto_size_id_games = GetCookie('auto_size_'+id_games);
	if(test == 1) alert('Autosize > cookie auto_size - auto_size_id_games : '+auto_size_id_games);

	if (auto_size_id_games) {
    	//document.getElementById('size').innerHTML='<span style="save_load"><strong><a href="/" title="Save">'+le_prenom+' !</a></strong><br></span>'; 
		b=auto_size_id_games.split('|');
		var id_games = b[0];
		var auto_size = b[1];
		
		if(test == 1) alert('Autosize > cookie extracted - id_games : '+id_games+' - auto_size : '+auto_size);
		
		if(auto_size == 1) {
			if(test == 1) alert('Autosize > auto_size is "On" and go "Off" > Game size don\'t change');
			// Update cookies
			var concatenation_autosize_on_off = id_games+"|0";
			var pathname=location.pathname;
			var myDomain=pathname.substring(0,pathname.lastIndexOf('/')) +'/';
			var date_exp = new Date();
			date_exp.setTime(date_exp.getTime()+(3650*24*3600*1000));
			SetCookie('auto_size_'+id_games,concatenation_autosize_on_off,date_exp,myDomain);	
			// Auto_size = "off"
			document.getElementById("survol_zauto_id").className="survol_zauto";
		}
		else {
			if(test == 1) alert('Autosize > auto_size is "Off" and go "On" > Fullsize - anim - id_games : '+id_games+' - game_size_width_dev : '+game_size_width_dev+' - game_size_height_dev : '+game_size_height_dev);
			
			// Add cookies
			var concatenation_autosize_on_off = id_games+"|1";
			var pathname=location.pathname;
			var myDomain=pathname.substring(0,pathname.lastIndexOf('/')) +'/';
			var date_exp = new Date();
			date_exp.setTime(date_exp.getTime()+(3650*24*3600*1000));
			SetCookie('auto_size_'+id_games,concatenation_autosize_on_off,date_exp,myDomain);	
			
			if(test == 1) alert('Autosize > Fullsize call ...');
			Fullsize('anim',id_games,game_size_width_dev,game_size_height_dev);	
		
			// Bouton auto_size is "on"
			if(test == 1) alert('Autosize > Background style is now change to "on".');
		
			document.getElementById("survol_zauto_id").className="survol_zauto_on";
		}
		
	}
	else { // Cookie don't exists : auto_size > "On"		

		if(test == 1) alert('Autosize > cookie don\'t exists : Autosize auto_size is "Off" and go "On"');

		// Add cookies
		var concatenation_autosize_on_off = id_games+"|1";
		var pathname=location.pathname;
		var myDomain=pathname.substring(0,pathname.lastIndexOf('/')) +'/';
		var date_exp = new Date();
		date_exp.setTime(date_exp.getTime()+(3650*24*3600*1000));
		SetCookie('auto_size_'+id_games,concatenation_autosize_on_off,date_exp,myDomain);	
		
		Fullsize('anim',id_games,game_size_width_dev,game_size_height_dev);	
		
	}

}



// Jeux-site.php > Formats
// ==========================================================================================================================================

function MyFormatAjax(Id, Format, Ext, To14) {
  var OAjax;
  if (window.XMLHttpRequest) OAjax = new XMLHttpRequest();
  else if (window.ActiveXObject) OAjax = new ActiveXObject('Microsoft.XMLHTTP');
  OAjax.open('POST',"jeux-site-RequPHP.php",true); // A FAIRE : chemin ok ou ../../include... ?
  OAjax.onreadystatechange = function()
  {
      if (OAjax.readyState == 4 && OAjax.status==200)
      {
          if (document.getElementById)
          {   
              if (OAjax.responseText =='true') { /* OK */
                    document.getElementById('msg').innerHTML=''+OAjax.responseText+'';
              }
			  else{                             /* PAS OK */
                    document.getElementById('msg').innerHTML=''+OAjax.responseText+'';
              }
          }     
      }
  }
  OAjax.setRequestHeader("Content-Type", "application/x-www-form-urlencoded; charset=ISO-8859-1");
  OAjax.send('Id='+Id+'&Format='+Format+'&Ext='+Ext+'&To14='+To14);  
} 




// Jeux > Envoyer ami
// ==========================================================================================================================================

url = document.location.href;
xend = url.lastIndexOf("/") + 1;
var base_url = url.substring(0, xend);
var ajax_get_error = false;

function ajax_do(url) {
   // Does URL begin with http?
   if (url.substring(0, 4) != 'http') {
      url = base_url + url;
   }
   // Create new JS element
   var jsel = document.createElement('SCRIPT');
   jsel.type = 'text/javascript';
   jsel.src = url;
   // Append JS element (therefore executing the 'AJAX' call)
   document.body.appendChild (jsel);
   return true;
}



function ajax_get(url, el) {
   // Has element been passed as object or id-string?
   if (typeof(el) == 'string') {
      el = document.getElementById(el);
   }
   // Valid el?
   if (el == null) { return false; }
   // Does URL begin with http?
   if (url.substring(0, 4) != 'http') {
      url = base_url + url;
   }
   // Create getfile URL
   getfile_url = base_url + '../include/p_all/getfile.php?url=' + escape(url) + '&el=' + escape(el.id);
   // Do Ajax
   ajax_do (getfile_url);
   return true;
}



function validEmail(emailami) { // VERIFICATION DE LA SYNTHAXE DE L'EMAIL // FAIRE UNE SULE FONCTION <-> validEmail()
   invalidChars = "/:,;~"
   // verifie qu'il n'y a pas de caracteres pas autorises
   for (i=0; i<invalidChars.length; i++) {
   badChar = invalidChars.charAt(i)
   if (emailami.indexOf(badChar,0) > -1) {
   return false
   }
   }
   // verifie qu'il y a un @
   atPos = emailami.indexOf("@",1)
   if (atPos == -1) {
   return false
   }
   // et seulement un @
   if (emailami.indexOf("@", atPos+1) != -1) {
   return false
   }
   // et au moins un point apres le @
   periodPos = emailami.indexOf(".",atPos)
   if (periodPos == -1) {
   return false
   }
   //verifie qu'il y a au moins un caractère entre le @ et le .
   if (periodPos - atPos < 2) {
   return false
   }
   //verifie qu'il y a au moins deux caracteres apres le point
   if (periodPos+3 > emailami.length) {
   return false
   }
   return true
}



function valeurCheck() {
	
	if(test == 4) alert('JS > valeurCheck()');
   
	var bloque = 0;
	
	var reco_name = document.getElementById('reco_name').value;
	var reco_email = document.getElementById('reco_email').value;
	var reco_email_ami = document.getElementById('reco_email_ami').value;
	var reco_comment = document.getElementById('reco_comment').value;
	var jeu_id = document.getElementById('jeu_id').value;
	var jeu_nom = document.getElementById('jeu_nom').value;
	var jeu_cat = document.getElementById('jeu_cat').value;
	var jeu_ref_cat = document.getElementById('jeu_ref_cat').value;
	var jeu_image = document.getElementById('jeu_image').value;
	var jeu_ip = document.getElementById('jeu_ip').value;
	var jeu_total = document.getElementById('jeu_total').value;
	var jeu_to14 = document.getElementById('jeu_to14').value;

		
   // test
   if(test == 4) alert(reco_name+' - '+reco_email+' - '+reco_email_ami+' - '+reco_comment+' - '+jeu_id+' - '+jeu_nom+' - '+jeu_cat+' - '+jeu_ref_cat+' - '+jeu_image+' - '+jeu_ip+' - '+jeu_total+' - '+jeu_to14+' - ');

   /* 
   // CHECK MESSAGE -- on commence par en bas pour que le onfocus replace la page au bon niveau d'erreur
   if(comment == "" ){ // si champ comment vide
      document.getElementById('comment').focus(); // focus dans le champ
      document.getElementById('comment').style.border = '1px solid #D41919'; // on change la couleur du border du INPUT
      document.getElementById('commentbox').innerHTML = '<font style="color:#cc0000; font-size:10px;"><img src="reco\/images\/no.gif" alt="" \/> Veuillez entrer votre message.<\/font>'; // on affichage le message dans le div ID
   }else{ // si champ comment rempli
      document.getElementById('comment').style.border = '1px solid #CFD8E0'; // on remet la couleur du border du INPUT
      document.getElementById('commentbox').innerHTML = '&nbsp;<img src="reco\/images\/yes.gif" alt="" \/>'; // on affiche une image OK
   }
   */
   
   
	// CHECK COMMENTAIRE
	document.getElementById("reco_comment").className = "envoyer_ami_contenu yes_grey";
	if(reco_comment == ""){
		document.getElementById("reco_comment").className = "envoyer_ami_contenu yes_grey";
	}
	else{
		document.getElementById('reco_comment').style.border = '1px solid #CFD8E0';
		document.getElementById("reco_comment").className = "envoyer_ami_contenu yes";
	}


	// CHECK EMAIL AMI
	if(reco_email_ami == "" ){
		document.getElementById('reco_email_ami').focus();
		document.getElementById('reco_email_ami').style.border = '1px solid #D41919';
		document.getElementById("reco_email_ami").className = "envoyer_ami_contenu nearly_yes";
		document.getElementById('reco_email_ami_msg').innerHTML = '<font style="color:#cc0000; font-size:10px; padding-left:5px;">Veuillez entrer l\'email de votre ami.<\/font>';
		var bloque = 1;
	}
	else if (!validEmail(reco_email_ami)) {
		document.getElementById('reco_email_ami').focus();
		document.getElementById('reco_email_ami').select();
		document.getElementById('reco_email_ami').style.border = '1px solid #D41919';
		document.getElementById("reco_email_ami").className = "envoyer_ami_contenu nearly_yes";
		document.getElementById('reco_email_ami_msg').innerHTML = '<font style="color:#cc0000; font-size:10px; padding-left:5px;">L\'email de votre ami n\'est pas valide<\/font>';
		var bloque = 1;
	}
	else{
		document.getElementById('reco_email_ami').style.border = '1px solid #CFD8E0';
		document.getElementById("reco_email_ami").className = "envoyer_ami_contenu yes";
		document.getElementById('reco_email_ami_msg').innerHTML = '';
	}
	
	
	// CHECK EMAIL 
	if(reco_email == "" ){
		document.getElementById('reco_email').focus();
		document.getElementById('reco_email').style.border = '1px solid #D41919';
		document.getElementById("reco_email").className = "envoyer_ami_contenu nearly_yes";
		document.getElementById('reco_email_msg').innerHTML = '<font style="color:#cc0000; font-size:10px; padding-left:5px;">Veuillez entrer votre email.<\/font>';
		var bloque = 1;
	}
	else if (!validEmail(reco_email)) {
		document.getElementById('reco_email').focus();
		document.getElementById('reco_email').select();
		document.getElementById('reco_email').style.border = '1px solid #D41919';
		document.getElementById("reco_email").className = "envoyer_ami_contenu nearly_yes";
		document.getElementById('reco_email_msg').innerHTML = '<font style="color:#cc0000; font-size:10px; padding-left:5px;">Votre email n\'est pas valide<\/font>';
		var bloque = 1;
	}
	else{
		document.getElementById('reco_email').style.border = '1px solid #CFD8E0';
		document.getElementById("reco_email").className = "envoyer_ami_contenu yes";
		document.getElementById('reco_email_msg').innerHTML = '';
	}
	
	// CHECK NOM
	if(reco_name == "" ){
		document.getElementById('reco_name').focus();
		document.getElementById('reco_name').style.border = '1px solid #D41919';
		document.getElementById("reco_name").className = "envoyer_ami_contenu nearly_yes";
		document.getElementById('reco_name_msg').innerHTML = '<font style="color:#cc0000; font-size:10px; padding-left:5px;">Veuillez entrer votre nom.<\/font>';
		var bloque = 1;
	}
	else{
		document.getElementById('reco_name').style.border = '1px solid #CFD8E0';
		document.getElementById("reco_name").className = "envoyer_ami_contenu yes";
		document.getElementById('reco_name_msg').innerHTML = '';
	}
	
	// CHECK TITRE JEU
	/*
	if(reco_name == "" ){
		document.getElementById('contact_jeu_titre').focus();
		document.getElementById('contact_jeu_titre').style.border = '1px solid #D41919';
		document.getElementById("contact_jeu_titre").className = "envoyer_ami_contenu nearly_yes";
		document.getElementById('contact_jeu_titre_msg').innerHTML = '<font style="color:#cc0000; font-size:10px; padding-left:5px;">Veuillez le titre du jeu.<\/font>';
	}
	else{
		document.getElementById('contact_jeu_titre').style.border = '1px solid #CFD8E0';
		document.getElementById("contact_jeu_titre").className = "envoyer_ami_contenu yes";
		document.getElementById('contact_jeu_titre_msg').innerHTML = '';
	}
	*/
	

	if(test == 4) alert('valeurCheck() > reco_name:'+reco_name+' - reco_email:'+reco_email+' - reco_email_ami:'+reco_email_ami+' - reco_comment:'+reco_comment+' - jeu_id:'+jeu_id+' - jeu_nom:'+jeu_nom+' - jeu_cat:'+jeu_cat+' - jeu_ref_cat:'+jeu_ref_cat+' - jeu_image:'+jeu_image+' - jeu_ip:'+jeu_ip+' - jeu_total:'+jeu_total+' - jeu_to14:'+jeu_to14);
  
	if(bloque == 0) {
		document.getElementById("reco_email_ami").value="";
		//document.getElementById('result_msg').innerHTML = '&nbsp;<img src="images\/yes.gif" alt="" \/> Message envoyé !';
		document.getElementById("button_send_reco").value = "Message envoyé !";
		document.getElementById("button_send_reco").className = "buttonjc_2_ok";
		//document.getElementById("button_send_reco").disabled = true;
		
		if(test == 4) alert('AjaxMailRecoSend : avant - bloque:'+0);
		if(bloque == 0) AjaxMailRecoSend(reco_name, reco_email, reco_email_ami, reco_comment, jeu_id, jeu_nom, jeu_cat, jeu_ref_cat, jeu_image, jeu_ip, jeu_total, jeu_to14);

	}
	
	document.getElementById('reco_comment').onfocus=function(){
		document.getElementById("button_send_reco").className = "buttonjc_2";
		document.getElementById("button_send_reco").value = "Envoyer le jeu";
	};
	document.getElementById('reco_email_ami').onfocus=function(){
		document.getElementById("button_send_reco").className = "buttonjc_2";
		document.getElementById("button_send_reco").value = "Envoyer le jeu";
	};
	document.getElementById('reco_email').onfocus=function(){
		document.getElementById("button_send_reco").className = "buttonjc_2";
		document.getElementById("button_send_reco").value = "Envoyer le jeu";
	};
	document.getElementById('reco_name').onfocus=function(){
		document.getElementById("button_send_reco").className = "buttonjc_2";
		document.getElementById("button_send_reco").value = "Envoyer le jeu";
	};
	
}



// Envoie du mail en Ajax
function AjaxMailRecoSend(reco_name, reco_email, reco_email_ami, reco_comment, jeu_id, jeu_nom, jeu_cat, jeu_ref_cat, jeu_image, jeu_ip, jeu_total, jeu_to14) {
	
	if(test == 4) alert('function AjaxMailRecoSend()');
	
	var OAjax;
	
	if (window.XMLHttpRequest) OAjax = new XMLHttpRequest();
	else if (window.ActiveXObject) OAjax = new ActiveXObject('Microsoft.XMLHTTP');
	OAjax.open('POST',"include/p_jeux/mail-reco-send.php",true);

	if(grecaptcha.getResponse() == '') {
		document.getElementById("button_send_reco").className = "buttonjc_2";
		document.getElementById("button_send_reco").value = "Envoyer le jeu";
	}
		
	OAjax.onreadystatechange = function()
	{

		if (OAjax.readyState == 4 && OAjax.status==200)
		  {
			  if (document.getElementById)
			  {   
			  
				  if(test == 4) alert('OAjax.responseText: '+OAjax.responseText);

				  if (OAjax.responseText =='true') { /* OK */
						/*document.getElementById('msg').innerHTML='<font color=GREEN>'+OAjax.responseText+'</font>';*/
						document.getElementById('msg_reco_envoie').innerHTML='<font color=GREEN>'+OAjax.responseText+'</font>';
				  }
				  else{                             /* PAS OK */
						/*document.getElementById('msg').innerHTML='<font color=RED>'+OAjax.responseText+'</font>';*/
						document.getElementById('msg_reco_envoie').innerHTML='<font color=RED>'+OAjax.responseText+'</font>';
				  }
				  
			  }     
		  }
	}
	OAjax.setRequestHeader("Content-Type", "application/x-www-form-urlencoded; charset=ISO-8859-1");
		
	OAjax.send('reco_name='+reco_name+'&reco_email='+reco_email+'&reco_email_ami='+reco_email_ami+'&reco_comment='+reco_comment+'&jeu_id='+jeu_id+'&jeu_nom='+jeu_nom+'&jeu_cat='+jeu_cat+'&jeu_ref_cat='+jeu_ref_cat+'&jeu_image='+jeu_image+'&jeu_ip='+jeu_ip+'&jeu_total='+jeu_total+'&jeu_to14='+jeu_to14+'&g-recaptcha-response='+grecaptcha.getResponse()); 
	
	if(test == 4) alert('reco_name='+reco_name+'&reco_email='+reco_email+'&reco_email_ami='+reco_email_ami+'&reco_comment='+reco_comment+'&jeu_id='+jeu_id+'&jeu_nom='+jeu_nom+'&jeu_cat='+jeu_cat+'&jeu_ref_cat='+jeu_ref_cat+'&jeu_image='+jeu_image+'&jeu_ip='+jeu_ip+'&jeu_total='+jeu_total+'&jeu_to14='+jeu_to14+'&g-recaptcha-response='+grecaptcha.getResponse());
	if(test == 4) alert('OK!');

} 



// Contact
function valeurCheck_contact() {
	
	if(test == 1) alert('function valeurCheck_contact()');
	
	var contact_name = document.getElementById('contact_name').value;
	
	var contact_email = document.getElementById('contact_email').value;
	var contact_sujet = document.querySelector('input[name="contact_sujet"]:checked').value; // A FAIRE : Marche sur tous les navigateurs ?
	
	var contact_message = document.getElementById('contact_message').value;

	var contact_jeu_id = document.getElementById('contact_jeu_id').value;
	
	var contact_ip = document.getElementById('contact_ip').value;
	var contact_navigateur = navigator.userAgent.toLowerCase();

	var contact_jeu_titre = document.getElementById('contact_jeu_titre').value;
	var contact_jeu_description = document.getElementById('contact_jeu_description').value;
	var contact_jeu_swf = document.getElementById('contact_jeu_swf').value;
	var contact_jeu_image = document.getElementById('contact_jeu_image').value;


	/* Sujet : proposer un jeu */
	if(test == 1) alert('valeurCheck_contact() > contact_name:'+contact_name+' - contact_email:'+contact_email+' - contact_sujet:'+contact_sujet+' - contact_message:'+contact_message+' - contact_jeu_id:'+contact_jeu_id+' - contact_ip:'+contact_ip+' - contact_navigateur:'+contact_navigateur+' - contact_jeu_titre:'+contact_jeu_titre+' - contact_jeu_description:'+contact_jeu_description+' - contact_jeu_swf:'+contact_jeu_swf+' - contact_jeu_image:'+contact_jeu_image); 
 	
  
	var bloque = 0;
	
	// A FAIRE 0 : Secu des saisies

	// CHECK > Email 
	//alert('contact_email:'+contact_email);
	if(contact_email == "" || contact_email == "Votre adresse email"){
		document.getElementById('contact_email').focus();
		document.getElementById('contact_email').style.border = '1px solid #D41919';
		document.getElementById("contact_email").className = "form_contact_input nearly_yes_contact";
		document.getElementById('contact_email_msg').innerHTML = '<span class="contact_error_message">Veuillez entrer votre email.</span>';
		var bloque = 1;
	}
	else if (!validEmail(contact_email)) {
		document.getElementById('contact_email').focus();
		document.getElementById('contact_email').select();
		document.getElementById('contact_email').style.border = '1px solid #D41919';
		document.getElementById("contact_email").className = "form_contact_input nearly_yes_contact";
		document.getElementById('contact_email_msg').innerHTML = '<span class="contact_error_message">Votre email n\'est pas valide.</span>';
		var bloque = 1;
	}
	else{
		document.getElementById('contact_email').style.border = '1px solid #CFD8E0';
		document.getElementById("contact_email").className = "form_contact_input yes_contact";
		document.getElementById('contact_email_msg').innerHTML = '';
	}
	
	
	
	// CHECK > Nom
	if(contact_name == "" || contact_name == "Votre nom"){
		document.getElementById('contact_name').focus();
		document.getElementById('contact_name').style.border = '1px solid #D41919';
		document.getElementById("contact_name").className = "form_contact_input nearly_yes_contact";
		document.getElementById('contact_name_msg').innerHTML = '<span class="contact_error_message">Veuillez entrer votre nom.</span>';
		var bloque = 1;
	}
	else{
		document.getElementById('contact_name').style.border = '1px solid #CFD8E0';
		document.getElementById("contact_name").className = "form_contact_input yes_contact";
		document.getElementById('contact_name_msg').innerHTML = '';
	}
	
	// A FAIRE 1 : activer la saisie semi auto
			



	// PROPOSER UN JEU
	// --------------------------------------------------------------------------------------------------------------------------------
	if(contact_sujet == 2) {

		// CHECK > Message A FAIRE ajouter hidden message dans proposer un jeu
		if(contact_message != 0) {
			if(contact_message == ""){
				document.getElementById('contact_message').focus();
				document.getElementById('contact_message').style.border = '1px solid #D41919';
				document.getElementById("contact_message").className = "form_contact_input nearly_yes_contact";
				document.getElementById('contact_message_msg').innerHTML = '<span class="contact_error_message">Veuillez saisir votre message.</span>';
				var bloque = 1;
			}
			else{
				document.getElementById('contact_message').style.border = '1px solid #CFD8E0';
				document.getElementById("contact_message").className = "form_contact_input yes_contact";
				document.getElementById('contact_message_msg').innerHTML = '';
			}
		}
	
	
	
		// CHECK > Proposer un jeu > Titre du jeu
		if(test == 1) alert('contact_jeu_titre:'+contact_jeu_titre);
		if(contact_jeu_titre == ""){
			document.getElementById("contact_jeu_titre").className = "form_contact_input nearly_yes_contact";
			document.getElementById("contact_jeu_titre").className = "form_contact_input nearly_yes_contact";
			document.getElementById("contact_jeu_titre_msg").innerHTML = '<span class="contact_error_message">Veuillez saisir le titre de ce jeu.</span>';
			var bloque = 1;
		}
		else{
			document.getElementById("contact_jeu_titre").style.border = '1px solid #CFD8E0';
			document.getElementById("contact_jeu_titre").className = "form_contact_input yes_contact";
			document.getElementById("contact_jeu_titre").innerHTML = '';
			document.getElementById("contact_jeu_titre_msg").innerHTML = '';
		}
		
		// CHECK > Proposer un jeu > Description du jeu
		/*
		alert('contact_jeu_description:'+contact_jeu_description);
		if(contact_jeu_description == "" || contact_jeu_description == "http://"){
			document.getElementById("contact_jeu_description").className = "form_contact_input nearly_yes_contact";
			document.getElementById("contact_jeu_description").className = "form_contact_input nearly_yes_contact";
			document.getElementById("contact_jeu_description_msg").innerHTML = '<span class="contact_error_message">Veuillez saisir une description UNIQUE pour ce jeu.</span>';
			var bloque = 1;
		}
		else{
			document.getElementById("contact_jeu_description").style.border = '1px solid #CFD8E0';
			document.getElementById("contact_jeu_description").className = "form_contact_input yes_contact";
			document.getElementById("contact_jeu_description").innerHTML = '';
			document.getElementById("contact_jeu_description_msg").innerHTML = '';
		}
		*/
	
		// CHECK > Proposer un jeu > Fichier du jeu
		if(contact_jeu_swf != 0) {
			if(contact_jeu_swf == "" || contact_jeu_swf == "http://"){	// A FAIRE 5 : vérifier que l'URL est correcte
			document.getElementById("contact_jeu_swf").className = "form_contact_input nearly_yes_contact";
			document.getElementById("contact_jeu_swf").className = "form_contact_input nearly_yes_contact";
			document.getElementById('contact_jeu_swf_msg').innerHTML = '<span class="contact_error_message">Veuillez saisir l\'URL du fichier jeu.</span>';
			var bloque = 1;
		}
		else{
			document.getElementById('contact_jeu_swf').style.border = '1px solid #CFD8E0';
			document.getElementById("contact_jeu_swf").className = "form_contact_input yes_contact";
			document.getElementById('contact_jeu_swf').innerHTML = '';
			document.getElementById('contact_jeu_swf_msg').innerHTML = '';
			}
		}
		
		// CHECK > Proposer un jeu > Image du jeu
		if(contact_jeu_image != 0) {
			if(contact_jeu_image == "" || contact_jeu_image == "http://"){// A FAIRE 5 : vérifier que l'URL est correcte
			document.getElementById("contact_jeu_image").className = "form_contact_input nearly_yes_contact";
			document.getElementById("contact_jeu_image").className = "form_contact_input nearly_yes_contact";
			document.getElementById('contact_jeu_image_msg').innerHTML = '<span class="contact_error_message">Veuillez saisir l\'URL de votre image.</span>';
			var bloque = 1;
		}
		else{
			document.getElementById('contact_jeu_image').style.border = '1px solid #CFD8E0';
			document.getElementById("contact_jeu_image").className = "form_contact_input yes_contact";
			document.getElementById('contact_jeu_image_msg').innerHTML = '';
			document.getElementById('contact_jeu_image_msg').innerHTML = '';
			}
		}
		
	}
	// --------------------------------------------------------------------------------------------------------------------------------



	
	// CHECK > Message
	if(contact_sujet != 2) {		
		if(contact_message == ""){
			document.getElementById("contact_message").className = "form_contact_input_message nearly_yes_contact";
			document.getElementById("contact_message").className = "form_contact_input_message nearly_yes_contact";
			document.getElementById('contact_message_msg').innerHTML = '<span class="contact_error_message">Veuillez entrer votre message.</span>';
			var bloque = 1;
		}
		else{
			document.getElementById('contact_message').style.border = '1px solid #CFD8E0';
			document.getElementById("contact_message").className = "form_contact_input_message yes_contact";
			document.getElementById('contact_message_msg').innerHTML = '';
		}
	}

	if(test == 1) alert('Contenu bloqué ? (1 = oui) :'+bloque);
		
	if(bloque == 0) AjaxMailSend(contact_name, contact_email, contact_sujet, contact_message, contact_jeu_id, contact_ip, contact_navigateur, contact_jeu_titre, contact_jeu_description, contact_jeu_swf, contact_jeu_image);
	//if(bloque == 0) AjaxMailSend(contact_name, contact_email, contact_sujet, contact_message, contact_jeu_id, contact_ip, contact_jeu_titre, contact_jeu_description, contact_jeu_swf, contact_jeu_image);

	if(test == 1) {
		if(bloque == 0) alert('Message envoyé !');
		else alert('Message NON envoyé !');
	}
		
	// MAJ du bouton
	if(bloque == 0 && contact_jeu_titre != 0){
		document.getElementById("button_send_contact").value = "Jeu envoyé !";
		document.getElementById("button_send_contact").disabled = true;
		document.getElementById("button_send_contact").className = "buttonjc_2_ok";
	}
	else if (bloque == 0 && contact_jeu_titre == 0){
		document.getElementById("button_send_contact").value = "Message envoyé !";
		document.getElementById("button_send_contact").disabled = true;
		document.getElementById("button_send_contact").className = "buttonjc_2_ok";
	}
   
}



// Envoie du mail en Ajax
//function AjaxMailSend(contact_name, contact_email, contact_sujet, contact_message, contact_jeu_id, contact_ip, contact_jeu_titre, contact_jeu_description, contact_jeu_swf, contact_jeu_image) {

function AjaxMailSend(contact_name, contact_email, contact_sujet, contact_message, contact_jeu_id, contact_ip, contact_navigateur, contact_jeu_titre, contact_jeu_description, contact_jeu_swf, contact_jeu_image) {
	
	var OAjax;
	
	if (window.XMLHttpRequest) OAjax = new XMLHttpRequest();
	else if (window.ActiveXObject) OAjax = new ActiveXObject('Microsoft.XMLHTTP');
	OAjax.open('POST',"../../include/p_all/mail-send.php",true);
	OAjax.onreadystatechange = function()
	{
		//alert('OAjax.responseText: '+OAjax.responseText);

		if (OAjax.readyState == 4 && OAjax.status==200)
		  {
			  if (document.getElementById)
			  {   
				  if (OAjax.responseText =='true') { /* OK */
					    if(test == 1) alert('OAjax.responseText = true');
						/*document.getElementById('msg').innerHTML='<font color=GREEN>'+OAjax.responseText+'</font>';*/
						document.getElementById('msg_envoie').innerHTML='<font color=GREEN>'+OAjax.responseText+'</font>';
				  }
				  else{                             /* PAS OK */
					    if(test == 1) alert('OAjax.responseText = false');
						/*document.getElementById('msg').innerHTML='<font color=RED>'+OAjax.responseText+'</font>';*/
						document.getElementById('msg_envoie').innerHTML='<font color=RED>'+OAjax.responseText+'</font>';
				  }
				  
			  }     
		  }
	}
	
	OAjax.setRequestHeader("Content-Type", "application/x-www-form-urlencoded; charset=ISO-8859-1");
	
	//OAjax.send('contact_name='+contact_name+'&contact_email='+contact_email+'&contact_sujet='+contact_sujet+'&contact_message='+contact_message+'&contact_jeu_id='+contact_jeu_id+'&contact_ip='+contact_ip+'&contact_navigateur='+contact_navigateur+'&contact_jeu_titre='+contact_jeu_titre+'&contact_jeu_description='+contact_jeu_description+'&contact_jeu_swf='+contact_jeu_swf+'&contact_jeu_image='+contact_jeu_image); 
//	if(test == 1) alert(contact_name='+contact_name+'&contact_email='+contact_email+'&contact_sujet='+contact_sujet+'&contact_message='+contact_message+'&contact_jeu_id='+contact_jeu_id+'&contact_ip='+contact_ip+'&contact_jeu_titre='+contact_jeu_titre+'&contact_jeu_description='+contact_jeu_description+'&contact_jeu_swf='+contact_jeu_swf+'&contact_jeu_image='+contact_jeu_image+'');
	
	OAjax.send('contact_name='+contact_name+'&contact_email='+contact_email+'&contact_sujet='+contact_sujet+'&contact_message='+contact_message+'&contact_jeu_id='+contact_jeu_id+'&contact_ip='+contact_ip+'&contact_jeu_titre='+contact_jeu_titre+'&contact_navigateur='+contact_navigateur+'&contact_jeu_description='+contact_jeu_description+'&contact_jeu_swf='+contact_jeu_swf+'&contact_jeu_image='+contact_jeu_image); 
	
} 



