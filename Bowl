function SetCookie(name, value) {
	var argv=SetCookie.arguments;
	var argc=SetCookie.arguments.length;
	var expires=(argc > 2) ? argv[2] : null;
	var path=(argc > 3) ? argv[3] : null;
	//var domain=(argc > 4) ? argv[4] : null;
	var domain=window.location.hostname;
	if(domain == 'www.jeuxclic.com') domain=".jeuxclic.com";
	else if(domain == 'm.jeuxclic.com') domain=".jeuxclic.com";
	//var domain=".jeuxclic.com";
	var secure=(argc > 5) ? argv[5] : false;
	document.cookie=name+"="+escape(value)+
		((expires==null) ? "" : ("; expires="+expires.toGMTString()))+
		((path==null) ? "" : ("; path="+path))+
		((domain==null) ? "" : ("; domain="+domain))+
		((secure==true) ? "; secure" : "");
}



function getCookieVal(offset) {
	var endstr=document.cookie.indexOf (";", offset);
	if (endstr==-1) endstr=document.cookie.length;
	return unescape(document.cookie.substring(offset, endstr));
}



function GetCookie(name) {
	var arg=name+"=";
	var alen=arg.length;
	var clen=document.cookie.length;
	var i=0;
	while (i<clen) {
		var j=i+alen;
		if (document.cookie.substring(i, j)==arg) {
			return getCookieVal (j);
			if (test == 2) alert("GetCookie - Jeux id : " + getCookieVal(j));
		}
		i=document.cookie.indexOf(" ",i)+1;
		if (i==0) break;
	}	
	return null;
}




// Jeux > cookies "Mes jeux"  
// ==========================================================================================================================================
function SetCookie_mj(c_name,value,expiredays) {
	
	//var test = 2;
	
	if (test == 2) alert('SetCookie_mj > c_name:'+c_name+' - value:'+value+' - expiredays:'+expiredays);

	var exdate=new Date()
	exdate.setDate(exdate.getDate()+expiredays)
	var new_game_id = escape(value);
	if(isNaN(new_game_id) == true) return; // sécurité
	
	// 1- On vérifie les jeux déjà présents.
    var all_games = checkCookie_mj();

	// 2- On met ces jeux dans un tableau
	if(all_games != '') {
		
		if (test == 2) alert('1- Cookie existant - all_games : "'+all_games+'"');

		var all_games_tab = all_games.split("+");
		
		// Limite max
		var limite_mes_jeux_max = 50;
		var nb_mes_jeux = all_games_tab.length;
		var empties = all_games_tab.length - all_games_tab.filter(String).length;
		var all_games_tab_count = nb_mes_jeux - empties;
		/*
		if(all_games_tab_count > limite_mes_jeux_max) {
			if (test == 2) alert('1- Trop de jeux ('+all_games_tab_count+')');
			trop_de_jeux_confirmation();
		}
		else {
			if (test == 2) alert('1- Nb jeux ok ! ('+all_games_tab_count+')');
		}
		*/

		
			
		// On rajoute ce jeu
		if(all_games_tab.indexOf(new_game_id) > -1) {	 // A FAIRE : faire un fonction (utilisée dans "mes jeux" également ...)
			
			if (test == 2) alert('2- Position du jeu dans le tableau "all_games_tab" : '+all_games_tab.indexOf(new_game_id));

			delete all_games_tab[all_games_tab.indexOf(new_game_id)]; // Suppression dans le tableau
			var all_games = all_games_tab.join("+"); // On reconstruit le cookie
			// Suppression des + en début ou fin de chaîne.
			if(all_games.substring(0, 1) == "+") { var all_games = all_games.substring(1); }
			if(all_games.charAt(all_games.length-1) == "+") { var all_games = all_games.substring(0,all_games.length-1); }

			//document.cookie = c_name+ "=" +all_games+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString());
			var domain=window.location.hostname;
			if(domain == 'www.jeuxclic.com') domain=".jeuxclic.com";
			document.cookie = c_name+ "=" +all_games+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString() + ";domain="+domain+";path=/")
						
								
			if (test == 2) alert('2- Jeu suprimé de Mes Jeux');
			
			document.getElementById('ajouter_a_mes_jeux').style.backgroundPosition='0px 0px'; 
			document.getElementById('ajouter_a_mes_jeux').title = 'Ajouter dans "Mes Jeux"';
			
			supprimer_de_mes_jeux_confirmation();
			
			if (test == 2) alert('2-(supression) avant Ajax_Mj_DB : '+new_game_id+' - '+all_games+' - 1');

			if(all_games_tab_count <= 50) Ajax_Mj_DB(new_game_id,all_games,'1');
			//if (test == 2) alert('2- Update cookie - all_games : '+all_games);
			
		}
		else {
			
		    if(all_games_tab_count < limite_mes_jeux_max) {
			
				if (test == 2) alert('2- '+new_game_id+' est un nouveau jeu dans Mes Jeux');
				
				/* jeux vide ? */
				var tous_les_jeux_tab = readCookie('Visites');
				var tous_les_jeux_tab = tous_les_jeux_tab.split("+");
				var nb_mes_jeux = tous_les_jeux_tab.length;
				var empties = tous_les_jeux_tab.length - tous_les_jeux_tab.filter(String).length;
				var nb_mes_jeux_no_empties = nb_mes_jeux - empties;
				if(test == 2) alert('nb_mes_jeux:'+nb_mes_jeux+' - empties:'+empties+' = nb_mes_jeux_no_empties:'+nb_mes_jeux_no_empties+' - tous_les_jeux_tab:'+tous_les_jeux_tab);
				var nb_mes_jeux = nb_mes_jeux_no_empties;
	
	
				all_games_tab.push(new_game_id);
				var all_games = all_games_tab.join("+");	
				// Suppression des + en début ou fin de chaîne.
				if(all_games.substring(0, 1) == '+') { var all_games = all_games.substring(1); }
				if(all_games.charAt(all_games.length-1) == "+") { var all_games = all_games.substring(0,all_games.length-1); }
				
				//document.cookie = c_name+ "=" +all_games+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString())
				//document.cookie = c_name+ "=" +all_games+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString() + ";domain=.jeuxclic.com;path=/")

				var domain=window.location.hostname;
				if(domain == 'www.jeuxclic.com') domain=".jeuxclic.com";
				document.cookie = c_name+ "=" +all_games+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString() + ";domain="+domain+";path=/")
	
				
				if (test == 2) alert('2- Update cookie - all_games : '+all_games);
				
				document.getElementById('ajouter_a_mes_jeux').style.backgroundPosition='-16px 0'; 
				document.getElementById('ajouter_a_mes_jeux').title = 'Ce jeu fait parti de vos jeux favoris.';
				
				ajouter_a_mes_jeux_confirmation();
				
				if (test == 2) alert('2- (ajout) avant Ajax_Mj_DB : '+new_game_id+' - '+all_games+' - 1');
				
				if(all_games_tab_count <= 50) Ajax_Mj_DB(new_game_id,all_games,'0');
	
				// INSERTION SQL
				/*
				$ip = $_SERVER["HTTP_X_REMOTE_IP"];
				$today = date('Y-m-d H:i:s');
				mysql_query("INSERT INTO mesjeux(id_jeu,cookie,sup,ip,date) VALUES('$id','$Visites','0','$ip','$today')");
				*/
				//
			}
			else {
				if (test == 2) alert('2- '+all_games_tab_count+' est supérieur à la limite ('+limite_mes_jeux_max+'). Pas d\'ajout.');
				trop_de_jeux_confirmation();
			}
			
		}
		
	
	}
	else  { // Pas de cookie : première insertion
		
		if (test == 2) alert('1- Cookie n\'existe pas - all_games : "'+all_games+'"');


		var domain=window.location.hostname;
		if(domain == 'www.jeuxclic.com') domain=".jeuxclic.com";
		document.cookie = c_name+ "=" +value+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString() + ";domain="+domain+";path=/")

    	var all_games = checkCookie_mj();
		document.getElementById('ajouter_a_mes_jeux').style.backgroundPosition='-16px 0'; 
		document.getElementById('ajouter_a_mes_jeux').title = 'Ce jeu fait parti de vos jeux favoris.';
		ajouter_a_mes_jeux_confirmation();

		if (test == 2) alert('1.2- Pas de cookie > New cookie - all_games : '+all_games);

	}
	
    /*
	if (value) {
        var all_games_tab = new Array
        all_games_tab.push(value) + ' '; // Ajoute le nouveau prénom ainsi qu'un espace juste après
    }
	else {  
		//break; // On quitte la boucle
	}
	*/
	/*
	if (all_games_tab){
		for(var i = 0; i < all_games_tab.length; i++){                  
			if (test == 2) alert(all_games_tab); // Affiche les prénoms à la suite
		} 
	}
	*/

	//if (test == 2) alert('c_name : '+c_name+' - Jeux id : '+value);

}



function Ajax_Mj_DB(new_game_id,all_games,add_sup) { 
  
  if (test == 2) alert('Mes jeux (insertion SQL) - Ajax_Mj_DB : '+new_game_id+','+all_games+','+add_sup);
  
  if(all_games.length <= 450) {
	  
	  if (test == 2) alert('Mes jeux (insertion SQL) > Longueur de cookie OK ! ('+all_games.length+').');
  
	  var OAjax;
	  if (window.XMLHttpRequest) OAjax = new XMLHttpRequest();
	  else if (window.ActiveXObject) OAjax = new ActiveXObject('Microsoft.XMLHTTP');
	  OAjax.open('POST',"../../include/p_jeux/mesjeux_db_add_sup.php",true);
	  OAjax.onreadystatechange = function()
	  {
		  if (OAjax.readyState == 4 && OAjax.status==200)
		  {
			  if (document.getElementById)
			  {   
				  /**/
				  if (OAjax.responseText =='true') { // OK
						document.getElementById('msggmc').innerHTML='<font color=GREEN>'+OAjax.responseText+'</font>';
				  }
				  else{ // PAS OK
						document.getElementById('msggmc').innerHTML='<font color=RED>'+OAjax.responseText+'</font>';
				  }
				  
			  }     
		  }
	  }
	  OAjax.setRequestHeader("Content-Type", "application/x-www-form-urlencoded; charset=ISO-8859-1");
	  //OAjax.send('Id_cat='+Id_cat+'$Id_cat2='+Id_cat2);  
	  
	  if (test == 2) alert('Mes jeux (insertion SQL) - Ajax_Mj_DB : new_game_id='+new_game_id+'&all_games='+encodeURI(all_games)+'&add_sup='+add_sup);
	  
	  OAjax.send('new_game_id='+new_game_id+'&all_games='+encodeURI(all_games)+'&add_sup='+add_sup);  
	  //OAjax.send('new_game_id='+new_game_id);  
  }
  else {
	  if (test == 2) alert('Mes jeux (insertion SQL) > Longueur de cookie trop grande ('+all_games.length+').');
  }
} 



function SetCookie_mj_supprimer(c_name,value,expiredays) {

	var test = 0;

	var exdate=new Date()
	exdate.setDate(exdate.getDate()+expiredays)
	var new_game_id = escape(value);
	if(isNaN(new_game_id) == true) return; // sécurité
	
	// 1- On vérifie les jeux déjà présents.
    var all_games = checkCookie_mj();

	// 2- On met ces jeux dans un tableau
	if(all_games != '') {
		
		var all_games_tab = all_games.split("+");

		// On supprime ce jeu

		if(all_games_tab.indexOf(new_game_id) > -1) {	 // A FAIRE : faire un fonction (utilisée dans "mes jeux" également ...)

			delete all_games_tab[all_games_tab.indexOf(new_game_id)]; // Suppression dans le tableau
			
			var all_games = all_games_tab.join("+"); // On reconstruit le cookie
			// Suppression des + en début ou fin de chaîne.
			if(all_games.substring(0, 1) == '+') { var all_games = all_games.substring(1); }
			if(all_games.charAt(all_games.length-1) == "+") { var all_games = all_games.substring(0,all_games.length-1); }
			if(test == 2) alert('all_games_tab.join : "'+all_games+'"');
			
			var  all_games = all_games.replace(/ +(?= )/g,''); // On élimine les espaces en trop
			var  all_games = all_games.trim();
			if(test == 2) alert('all_games.replace spaces : "'+all_games+'"');

			var domain=window.location.hostname;
			if(domain == 'www.jeuxclic.com') domain=".jeuxclic.com";
			document.cookie = c_name+ "=" +all_games+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString() + ";domain="+domain+";path=/")
			
			//document.cookie = c_name+ "=" +all_games+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString());
			//document.cookie = c_name+ "=" +all_games+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString() + ";domain=.jeuxclic.com;path=/")

			document.getElementById('game_'+new_game_id+'').style.display = "none";

			if(test == 2) alert('SetCookie_mj_supprimer : "'+all_games+'"');
			
			//var tous_les_jeux_tab = readCookie('Visites'); //all_games et del cette ligne
			var  tous_les_jeux_tab = all_games.replace(/ +(?= )/g,'');
			var tous_les_jeux_tab = tous_les_jeux_tab.split("+");
			var nb_mes_jeux = tous_les_jeux_tab.length;
			
			/* jeux vide ? */
			var empties = tous_les_jeux_tab.length - tous_les_jeux_tab.filter(String).length;
			if(test == 2) alert('nb_mes_jeux:'+nb_mes_jeux+' - tous_les_jeux_tab:'+tous_les_jeux_tab);
			var nb_mes_jeux_no_empties = nb_mes_jeux - empties;
			if(test == 2) alert('nb_mes_jeux:'+nb_mes_jeux+' - empties:'+empties+' = nb_mes_jeux_no_empties:'+nb_mes_jeux_no_empties+' - tous_les_jeux_tab:'+tous_les_jeux_tab);
			var nb_mes_jeux = nb_mes_jeux_no_empties;
			
			// Car tous_les_jeux_tab.length est à 1 minimum
			if(tous_les_jeux_tab == "") var nb_mes_jeux = 0;
			
			//document.getElementById('mesjeux_nb_jeux').innerHTML='('+nb_mes_jeux+')';
			
			// A FAIRE 0 : gérer : if($nb_mes_jeux>99) $nb_mes_jeux = '+99';

			if(nb_mes_jeux == 1) {
				document.getElementById('filarianenbjeux').innerHTML='('+nb_mes_jeux+' jeu)';
				document.getElementById('mesjeux_nb_jeux').innerHTML='('+nb_mes_jeux+')';
			}
			else if(nb_mes_jeux > 1) {
				if(nb_mes_jeux >= 100) {
					document.getElementById('filarianenbjeux').innerHTML='(+99 jeux)';
					document.getElementById('mesjeux_nb_jeux').innerHTML='(+99)';
				}
				else {
					document.getElementById('filarianenbjeux').innerHTML='('+nb_mes_jeux+' jeux)';
					document.getElementById('mesjeux_nb_jeux').innerHTML='('+nb_mes_jeux+')';
				}
			}
			else {
				document.getElementById('filarianenbjeux').innerHTML='';
				document.getElementById('mesjeux_nb_jeux').innerHTML='';
			}
			
			if(nb_mes_jeux <= 50) Ajax_Mj_DB(new_game_id,all_games,'1');

			if(all_games == '') {
				if(userLang == 'fr-FR') document.getElementById("mesjeux_cookies_message").innerHTML = "<div class=\"mesjeux_vide\"><div align=\"left\">Pour ajouter et retrouver ici vos jeux favoris, cliquer sur l\'étoile <span class=\"mesjeux_tuto_star\"><\/span> présente à côté du titre du jeu.<\/div><br><div align=\"left\"><img src=\"images\/mes-jeux-tuto-etoile.png\" title=\"Page de jeux : ajouter à Mes Jeux\"><\/div><br><div align=\"left\"><img src=\"images\/mes-jeux-tuto-menu-bouton.png\" title=\"Bouton menu Mes Jeux\"><\/div><\/div>";
				else document.getElementById("mesjeux_cookies_message").innerHTML = "<div class=\"mesjeux_vide\"><div align=\"left\">To add and find your favorite games here, click on the star <span class=\"mesjeux_tuto_star\"><\/span> just after game title.<\/div><br><div align=\"left\"><img src=\"images\/mes-jeux-tuto-etoile.png\"><\/div><br><div align=\"left\"><img src=\"images\/mes-jeux-tuto-menu-bouton.png\"><\/div><\/div>";
			}

		}
	}
}


