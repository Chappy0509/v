		else document.getElementById('msg').innerHTML='<br><div class="form_contact_contenu"></span><br><br><div class="form_contact_titre"></div><table width="100%" border="0" cellspacing="0" cellpadding="0"><tr><td width="6%"></td><td width="94%"><textarea class="form_contact_input_message" name="contact_message" id="contact_message"></textarea> <span id="contact_message_msg"></span> </td></tr></table></div><br><input type="hidden" name="contact_jeu_description" id="contact_jeu_description" value="0" /><input type="hidden" name="contact_jeu_swf" id="contact_jeu_swf" value="0" /><input type="hidden" name="contact_jeu_image" id="contact_jeu_image" value="0" />';
	}
	else { /* Demande d'information */
		if(userLang == 'fr-FR') document.getElementById('msg').innerHTML='<br><p><label for="email" style="font-weight:bold; display:block; margin-bottom:1em;"><div class="form_contact_titre">Votre message</div></label></p><table width="100%" border="0" cellspacing="0" cellpadding="0"><tr><td width="6%"></td><td width="94%"><textarea class="form_contact_input_message" name="contact_message" id="contact_message"></textarea> <span id="contact_message_msg"></span></td></tr></table><br><br><input type="hidden" name="contact_id_jeu" id="contact_id_jeu" value="0" /><input type="hidden" name="contact_jeu_titre" id="contact_jeu_titre" value="0" /><input type="hidden" name="contact_jeu_description" id="contact_jeu_description" value="0" /><input type="hidden" name="contact_jeu_swf" id="contact_jeu_swf" value="0" /><input type="hidden" name="contact_jeu_image" id="contact_jeu_image" value="0" /><div class="g-recaptcha" data-sitekey="6LeWmxATAAAAAEloCoOh2bDFKcrMKM1zKHtbx8wO"></div>';
		else document.getElementById('msg').innerHTML='<br><p><label for="email" style="font-weight:bold; display:block; margin-bottom:1em;"><div class="form_contact_titre"></div></label></p><table width="100%" border="0" cellspacing="0" cellpadding="0"><tr><td width="6%"></td><td width="94%"><textarea class="form_contact_input_message" name="contact_message" id="contact_message"></textarea> <span id="contact_message_msg"></span></td></tr></table><br><br><input type="hidden" name="contact_id_jeu" id="contact_id_jeu" value="0" /><input type="hidden" name="contact_jeu_titre" id="contact_jeu_titre" value="0" /><input type="hidden" name="contact_jeu_description" id="contact_jeu_description" value="0" /><input type="hidden" name="contact_jeu_swf" id="contact_jeu_swf" value="0" /><input type="hidden" name="contact_jeu_image" id="contact_jeu_image" value="0" /><div class="g-recaptcha" data-sitekey="6LeWmxATAAAAAEloCoOh2bDFKcrMKM1zKHtbx8wO"></div>';
		
		document.getElementById('contact_sujet_infos').checked = true;
	}
	
	document.getElementById("button_send_contact").disabled = false;
}



<!-- Ajouter au favoris COOKIE -->
function favorisadd(id) {

var base_url = url.substring(0, xend);
url = base_url + 'include/p_jeux/cookies_add.php?id=' + escape(id);
newwindow=window.open(
url,'name2', 'status=no, toolbar=no, scrollbars=yes, titlebar=no, menubar=no, resizable=no, width=1, height=1, directories=no, location=no');

//document.getElementById('jeu_id_box').innerHTML='<strong>You changed me</strong>';
document.getElementById('jeuxajoute').innerHTML='<strong><a href="categorie-jeux.php?cat=mes-jeux" title="Afficher mes jeux favoris">OK</a> !</strong><br>';
// A FAIRE : jeux ajouté + lien + surbrillance lors de l'arrivé sur la page.

if (window.focus) {newwindow.focus()}
return false;

}



<!-- Supprimer des favoris COOKIE -->
function favorissup(id) {

var base_url = url.substring(0, xend);
url = base_url + 'include/p_jeux/cookies_sup.php?id='+ escape(id);
newwindow=window.open(
url,'name3', 'status=no, toolbar=no, scrollbars=yes, titlebar=no, menubar=no, resizable=no, width=1, height=1, directories=no, location=no');

//document.getElementById('jeu_id_box').innerHTML='<strong>You changed me</strong>';
//document.getElementById('jeuxsupprime').innerHTML='<strong>Jeu supprim&eacute; de vos jeux favoris</strong><br>';
// A mettre dans le code de la page de destination :
// <div id=\"jeuxsupprime\" style=\"display:inline;\">AFFICHAGE ICI</div>

if (window.focus) {newwindow.focus()}
return false;

}



// Cookie enabled or not ?
//===============================================
function are_cookies_enabled() {
    var cookieEnabled = (navigator.cookieEnabled) ? true : false;

    if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled)
    { 
        document.cookie="testcookie";
        cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;
    }
    return (cookieEnabled);
}



