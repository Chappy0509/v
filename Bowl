// Detect flash player
//===============================================
function detect_flash_player() {
	if(swfobject.hasFlashPlayerVersion("9.0.115")) {
		if(userLang == 'fr-FR') document.getElementById('flash_player_detection_msg').innerHTML='<div class="diagnostic_ok">Flash player est activé.</div>';
		else document.getElementById('flash_player_detection_msg').innerHTML='<div class="diagnostic_ok">Flash player not enabled.</div>';
	}
	else {
		if(userLang == 'fr-FR') document.getElementById('flash_player_detection_msg').innerHTML='<div class="diagnostic_notok">Merci de <a href="https://get.adobe.com/flashplayer/" rel="nofollow" target="_blank">mettre à jour Flash player</a>.</div>';
		else document.getElementById('flash_player_detection_msg').innerHTML='<div class="diagnostic_notok">Please <a href="https://get.adobe.com/flashplayer/" rel="nofollow" target="_blank">update Flash player</a>.</div>';
	}
}



// Sticky Ads
//===============================================
window.onscroll = function() {
    var sticky = document.getElementById('stickynav');
    if( document.body.scrollTop+document.documentElement.scrollTop > 135) {
        sticky.className = "stuck";
		//alert('>240');
	}
    else {
		sticky.className = "";
		//alert('>240');
	}
};



