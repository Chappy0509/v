// Index > First time visit > Setcookie
//===============================================
function first_time_visit(name,stop_msg) {
	
	var value = 1;
	var expiredays = 365;
	
	var exdate=new Date()
	exdate.setDate(exdate.getDate()+expiredays)
	// var new_game_id = escape(value);
	// if(isNaN(new_game_id) == true) return; // sécurité
	
	var cookie_exists;
    if(document.cookie.indexOf(name) == 0) cookie_exists = -1<document.cookie.indexOf(value?name+"="+value+";":name+"=");
	//document.cookie = c_name+ "=" +value+ ((expiredays == null) ? "" : ";expires="+exdate.toGMTString() + ";domain=.jeuxclic.com;path=/")
    //if(document.cookie.indexOf(name) == 0) cookie_exists = -1<document.cookie.indexOf(value?name+"="+value+";":name+"="+";domain=.jeuxclic.com;path=/");
    else if(value && document.cookie.indexOf("; "+name+"="+value) + name.length + value.length + 3== document.cookie.length) cookie_exists = 1;
    else { cookie_exists = -1<document.cookie.indexOf("; "+(value?name+"="+value + ";":name+"=")) }
	if(test == 3) alert('cookie_exists:'+cookie_exists);
	
	var cookie_first_time_visit = GetCookie(name);
	if(test == 3) alert('cookie_exists:'+cookie_first_time_visit);

	if(cookie_first_time_visit != '' && cookie_first_time_visit !== 'undefined' && cookie_first_time_visit !== 'null' && cookie_first_time_visit) {
		
		//if(!cookie_first_time_visit) alert('cookie_first_time_visit dont exists !!!'); // cas impossible
		
		if(test == 3) alert('cookie_exists:'+cookie_first_time_visit);
		
		var cookie_first_time_visit = cookie_first_time_visit.split('|');
		var nb_time_visited = cookie_first_time_visit[0];
		if(stop_msg == 0) var stop_msg = cookie_first_time_visit[1];
		nb_time_visited++;
		
		if (nb_time_visited > 0 && stop_msg == 0 && nb_time_visited < 15) {
	 
			//alert('cookie_first_time_visit:'+cookie_first_time_visit);
			//alert("nb_time_visited:" + nb_time_visited+' - stop_msg:'+stop_msg);
			// alert("Welcome again " + nb_time_visited);
			//nb_time_visited++;
			SetCookie(name, nb_time_visited+'|'+stop_msg);
			
			// A FAIRE 0 : if (nb_time_visited > 10) > Ne plus afficher.
			
			document.getElementById("premier_pas").className="info_box";	
			document.getElementById("premier_pas").innerHTML = "Premiers pas ? <a href=\"c.php?p=premiers-pas\" target=\"_blank\">Découvrez en 1 minute comment jouer sur Jeuxclic</a>. <a href=\"javascript:void(0)\" onClick=\"mask(\'premier_pas\');first_time_visit(\'first_time_visit\',1)\"><span class=\"close_button\"><\/span><\/a>";
			if(test == 3) alert("Welcome again (new nb_time_visited) " + nb_time_visited);
	
		}
		else {
			SetCookie(name, nb_time_visited+'|1');
		}
		
	}
	else {
		//if (username != 0 && username != null) {}
		SetCookie(name, "1|0"); 
	    if(test == 3) alert("Première visite !");
		document.getElementById("premier_pas").className="info_box";	
		document.getElementById("premier_pas").innerHTML = "Premiers pas ? <a href=\"c.php?p=premiers-pas\" target=\"_blank\">Découvrez en 1 minute comment jouer sur Jeuxclic</a>. <a href=\"javascript:void(0)\" onClick=\"mask(\'premier_pas\');first_time_visit(\'first_time_visit\',1)\"><span class=\"close_button\"><\/span><\/a>";
		
	}

}



// Index > First cookie > Setcookie
//===============================================
function first_time_cookie(name,stop_msg) {
	
	if(test == 5) alert('first_time_cookie()');
	
	var value = 1;
	var expiredays = 3650;
	
	var exdate=new Date()
	exdate.setDate(exdate.getDate()+expiredays)
	
	var cookie_exists;
    if(document.cookie.indexOf(name) == 0) cookie_exists = -1<document.cookie.indexOf(value?name+"="+value+";":name+"=");
    else if(value && document.cookie.indexOf("; "+name+"="+value) + name.length + value.length + 3== document.cookie.length) cookie_exists = 1;
    else { cookie_exists = -1<document.cookie.indexOf("; "+(value?name+"="+value + ";":name+"=")) }
	if(test == 5) alert('cookie_exists:'+cookie_exists);
	
	var cookie_first_time_cookie = GetCookie(name);
	if(test == 5) alert('cookie_exists:'+cookie_first_time_cookie);

	if(cookie_first_time_cookie != '' && cookie_first_time_cookie !== 'undefined' && cookie_first_time_cookie !== 'null' && cookie_first_time_cookie) {
		
		if(!cookie_first_time_cookie) if(test == 5) alert('cookie_first_time_cookie dont exists !!!');
		
		if(test == 5) alert('cookie_exists:'+cookie_first_time_cookie);
		
		var cookie_first_time_cookie = cookie_first_time_cookie.split('|');
		var nb_time_visited = cookie_first_time_cookie[0];
		if(stop_msg == 0) var stop_msg = cookie_first_time_cookie[1];
		nb_time_visited++;
		
		if (nb_time_visited > 0 && stop_msg == 0 && nb_time_visited < 1) {
	 
			//alert('cookie_first_time_cookie:'+cookie_first_time_cookie);
			//alert("nb_time_visited:" + nb_time_visited+' - stop_msg:'+stop_msg);
			// alert("Welcome again " + nb_time_visited);
			//nb_time_visited++;
			
			SetCookie(name, nb_time_visited+'|'+stop_msg);
			
			// A FAIRE 0 : if (nb_time_visited > 10) > Ne plus afficher.

			document.getElementById("premier_cookie").className="info_box";	
						document.getElementById("premier_cookie").innerHTML = "En continuant la navigation sur ce site, vous acceptez les cookies nous permettant d'améliorer votre expérience sur Jeuxclic. <a href=\"c.php?p=cgu#cookies\" target=\"_blank\">En savoir plus.</a> <a href=\"javascript:void(0)\" onClick=\"mask(\'premier_cookie\');first_time_cookie(\'first_time_cookie\',1)\"><span class=\"close_button\"><\/span><\/a>";
			if(test == 5) alert("Welcome again (new nb_time_visited) " + nb_time_visited);
	
		}
		else {
			SetCookie(name, nb_time_visited+'|1');
		}
		
	}
	else {
		//if (username != 0 && username != null) {}
		SetCookie(name, "1|0"); 
	    if(test == 3) alert("Premier cookie !");
		document.getElementById("premier_cookie").className="info_box";	
		document.getElementById("premier_cookie").innerHTML = "En continuant la navigation sur ce site, vous acceptez les cookies nous permettant d'améliorer votre expérience sur Jeuxclic. <a href=\"c.php?p=cgu#cookies\" target=\"_blank\">En savoir plus.</a> <a href=\"javascript:void(0)\" onClick=\"mask(\'premier_cookie\');first_time_cookie(\'first_time_cookie\',1)\"><span class=\"close_button\"><\/span><\/a>";
		
	}

	//setTimeout(function search_focus(){searchform.q.focus();},1);
	
}



// Menu langue
//===============================================
function menu_language_redirection(language,link_redirection) {
	alert('language:'+language+' - link_redirection:'+link_redirection);
	SetCookie('menu_language',language);
	location.href=link_redirection;
}


